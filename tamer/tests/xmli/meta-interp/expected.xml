    <classify as="@___dsgr_4f4@" />

    <param name="@___dsgr_51e@"
           desc="Generated from interpolated string `prefix-{@as@}-suffix`">
      <text>prefix-</text>
      <param-value name="@as@" />
      <text>-suffix</text>
    </param>
    <classify as="@___dsgr_51e@" />
